SiiNunit
{
# Fallback slave trailer used in case trailer data is missing to prevent crashes.

trailer : .fallback.slave
{
	accessories[]: .fallback.slave.tchassis
	accessories[]: .fallback.slave.trwheel0
	accessories[]: .fallback.slave.trwheel1
	accessories[]: .fallback.slave.paint_job

}

vehicle_accessory: .fallback.slave.tchassis
{
    	data_path: "/def/vehicle/trailer/scs_box/curtain_sider/chassis_ca.sii"
}

vehicle_wheel_accessory: .fallback.slave.trwheel0
{
	offset: 0
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .fallback.slave.trwheel1
{
	offset: 2
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_paint_job_accessory: .fallback.slave.paint_job
{
    	data_path: "/def/vehicle/trailer/scs_box/curtain_sider/company_paint_job/default.sii"
}

}
#eof
